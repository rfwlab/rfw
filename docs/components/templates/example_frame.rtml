<root>
  <div class="border rounded mb-4" data-code="@prop:code">
    <div class="flex border-b">
      <button id="tab-code" class="px-4 py-2 border-b-2 border-blue-500">Code</button>
      <button id="tab-preview" class="px-4 py-2">Preview</button>
    </div>
    <div id="code" class="p-4 font-mono text-sm overflow-auto"></div>
    <div id="preview" class="hidden">
      <iframe class="w-full h-64" src="@prop:uri"></iframe>
    </div>
  </div>
</root>
